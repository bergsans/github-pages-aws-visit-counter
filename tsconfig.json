{
  "compilerOptions": {
    "lib": ["ESNext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es5",
    "outDir": ".build",
    "rootDir": "./",
    "alwaysStrict": true
  }
}
